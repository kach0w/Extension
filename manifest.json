{
    "manifest_version": 2,
    "name": "Youtube Playlist Maker",
    "description": "Go to a Youtuber's page and click 'videos', then activate the extension and a list of all the videos will show up -- Extension Made by Kach0w",
    
    "version": "1.0",
    "browser_action": {
        "default_title": "TLH",
        "default_popup": "index.html",
        "icons": {
            "128": "Icon_128.png"
          }
        
  
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage"
        
    ],
    "content_scripts": [ 
        { 
            "js": [ "/node_modules/jquery/dist/jquery.min.js", "action.js", "content-script.js" ],
            "matches": ["*://*/*"]
        }
     ] 
}